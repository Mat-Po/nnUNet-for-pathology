{
    "wholeslidedata": {
        "default": {
            "yaml_source": "",
            "image_backend": "openslide",
            "labels": "",
            "annotation_parser": {
                "*object": "wholeslidedata.interoperability.qupath.parser.QuPathAnnotationParser",
                "sorters": [
                    "${$wholeslidedata.annotation.selector.sort_by_label_name_in_reference_list}"
                ]
            },
            "batch_shape": {
                "batch_size": "",
                "spacing": "",
                "shape": ""
            },
            "dataset": {
                "copy_path": "/home/user/data",
                "spacing": 0.5
            },
            "label_sampler_name": "WeightedLabelSampler",
            "label_sampler": {
                "labels": ""
            },
            "annotation_sampler_name": "AreaAnnotationSampler",
            "annotation_sampler": {
                "dataset": "${dataset}",
                "weight": 0.5
            },
            "batch_callbacks": [
                {
                    "*object": "nnunetv2.training.nnUNetTrainer.variants.pathology.wsd_pathology_DA_callback.nnUnetBatchCallback",
                        "patch_size_spatial": []},
                {
                    "*object": "wholeslidedata.samplers.callbacks.DeepSupervisionBatchCallback",
                        "sizes": []
                }
            ]
        }
    }
}
